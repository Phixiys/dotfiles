{
  "editor.fontFamily": "Dank Mono",
  "editor.fontSize": 14,
  "editor.lineHeight": 0,
  // "workbench.editor.tabSizing": "shrink",
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "editor.cursorBlinking": "solid",
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "none",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.colorDecorators": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.snippetSuggestions": "top",
  "editor.minimap.enabled": false,
  // Very important: Install this plugin: https://github.com/be5invis/vscode-custom-css
  // you'll need to change this to an absolute path on your computer
  "vscode_custom_css.imports": ["file:///Users/sasa/.vscodestyles.css"],
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.sideBar.location": "left",
  "editor.minimap.showSlider": "always",
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.next": true
  },
  "window.zoomLevel": 0,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.editor.tabCloseButton": "off",
  // Set the default
  "editor.formatOnSave": true,
  // Enable per-language
  "[javascript]": {
    "editor.formatOnSave": false
  },
  // this lets you quick open multiple items in a row and now have the previous ones closed on you
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "workbench.fontAliasing": "antialiased",
  "clock.dateFormat": "dddd mmm dd yyyy HH:MM",
  "explorer.confirmDelete": false,
  "workbench.activityBar.visible": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.statusBar.visible": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.port": 3000,
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "liveServer.settings.donotVerifyTags": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "editor.scrollBeyondLastLine": true,
  "editor.formatOnPaste": true,
  "gitlens.historyExplorer.enabled": true,
  "materialTheme.fixIconsRunning": false,
  "workbench.colorTheme": "Material Theme",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#84FFFF",
    "list.activeSelectionForeground": "#84FFFF",
    "list.inactiveSelectionForeground": "#84FFFF",
    "list.highlightForeground": "#84FFFF",
    "scrollbarSlider.activeBackground": "#84FFFF50",
    "editorSuggestWidget.highlightForeground": "#84FFFF",
    "textLink.foreground": "#84FFFF",
    "progressBar.background": "#84FFFF",
    "pickerGroup.foreground": "#84FFFF",
    "tab.activeBorder": "#84FFFF",
    "notificationLink.foreground": "#84FFFF",
    "editorWidget.resizeBorder": "#84FFFF",
    "editorWidget.border": "#84FFFF",
    "settings.modifiedItemForeground": "#84FFFF",
    "panelTitle.activeBorder": "#84FFFF"
  },
  "materialTheme.accentPrevious": "Bright Teal",
  "materialTheme.accent": "Sky",
  "javascript.updateImportsOnFileMove.enabled": "always"
}
